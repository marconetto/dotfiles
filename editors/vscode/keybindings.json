// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+=",
        "command": "extension.customWindowZoomIn"
    },
    {
        "key": "cmd+9",
        "command": "-extension.customWindowZoomIn"
    },
    {
        "key": "cmd+-",
        "command": "-workbench.action.zoomOut"
    },
    {
        "key": "cmd+-",
        "command": "extension.customWindowZoomOut"
    },
    {
        "key": "cmd+8",
        "command": "-extension.customWindowZoomOut"
    }
]